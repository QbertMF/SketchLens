{
  "expo": {
    "name": "SketchLens",
    "slug": "SketchLens",
    "version": "1.8.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": false,
      "permissions": [
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
        "WAKE_LOCK"
      ],
      "package": "de.qbert.SketchLens"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "5b5714fe-0b46-4fcc-b601-8d887d6d21ff"
      }
    },
    "plugins": [
      [
        "react-native-google-mobile-ads",
        {
          "android_app_id": "ca-app-pub-5939388532042811~8359733839",
          "androidAppId": "ca-app-pub-5939388532042811~8359733839",
          "ios_app_id": "ca-app-pub-5939388532042811~8359733839",
          "iosAppId": "ca-app-pub-5939388532042811~8359733839"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResourcesInReleaseBuilds": true,
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "manifestQueries": {
              "package": ["com.google.android.gms"]
            },
            "minSdkVersion": 21
          }
        }
      ]
    ]
  }
}
